    "@react-aria/datepicker": "workspace:",
    "@react-aria/dialog": "workspace:",
    "@react-aria/disclosure": "workspace:",
    "@react-aria/dnd": "workspace:",
    "@react-aria/focus": "workspace:",
    "@react-aria/gridlist": "workspace:",
    "@react-aria/i18n": "workspace:",
    "@react-aria/interactions": "workspace:",
    "@react-aria/label": "workspace:",
    "@react-aria/landmark": "workspace:",
    "@react-aria/link": "workspace:",
    "@react-aria/listbox": "workspace:",
    "@react-aria/menu": "workspace:",
    "@react-aria/meter": "workspace:",
    "@react-aria/numberfield": "workspace:",
    "@react-aria/overlays": "workspace:",
    "@react-aria/progress": "workspace:",
    "@react-aria/radio": "workspace:",
    "@react-aria/searchfield": "workspace:",
    "@react-aria/select": "workspace:",
    "@react-aria/selection": "workspace:",
    "@react-aria/separator": "workspace:",
